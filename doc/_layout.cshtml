<!DOCTYPE html>
<html>
  <head>
    <meta charset='utf-8'>
    <meta http-equiv="X-UA-Compatible" content="chrome=1">

    <link rel="stylesheet" type="text/css" href="@(Context.Settings[Keys.LinkRoot])/css/stylesheet.css" media="screen">
    <link rel="stylesheet" type="text/css" href="@(Context.Settings[Keys.LinkRoot])/css/pygment_trac.css" media="screen">
    <link rel="stylesheet" type="text/css" href="@(Context.Settings[Keys.LinkRoot])/css/print.css" media="print">

    <script src="//code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="@(Context.Settings[Keys.LinkRoot])/js/main.js"></script>
    
    <title>Space Battle Arena by Mikeware</title>
  </head>

  <body>

  @if(IsSectionDefined("Header"))
  {
    @RenderSection("Header")
  }
  else
  {
    @Html.Partial("_includes/header")
  }

    @Html.Partial("_includes/topnews")

    <div class="container">
      <section id="main_content">
        @RenderBody()
      </section>
    </div>

    @if(IsSectionDefined("Sections"))
					{
						@RenderSection("Sections")
    }

@section Footer {
    @Html.Partial("_includes/footer")
}  

</body>
</html>